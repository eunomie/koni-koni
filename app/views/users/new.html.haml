.container
  .row
    .col-md-6.col-md-offset-3
      .card
        .card-block
          %h4.card-title
            = t(".title")
          = form_for @user do |form|
            = render partial: '/users/form', object: form

            = form.submit

            .other-links
              = link_to t(".sign_in"), sign_in_path
